<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-downloader" version="0.0.1" 
    xmlns="http://apache.org/cordova/ns/plugins/1.0" 
    xmlns:android="http://schemas.android.com/apk/res/android">
    <name>Downloader</name>
    <js-module name="Downloader" src="www/Downloader.js">
        <clobbers target="cordova.plugins.Downloader" />
    </js-module>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="Downloader">
                <param name="android-package" value="cordova.plugin.downloader.Downloader" />
            </feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml"></config-file>
        <source-file src="src/android/Downloader.java" target-dir="src/cordova/plugin/downloader/Downloader" />
    </platform>
</plugin>